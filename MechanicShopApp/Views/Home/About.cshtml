
@{
    ViewBag.Title = "About";
    Layout = "~/Views/_LayoutPage1.cshtml";
}

<section class="w3l-about-breadcrumb">
    <div class="breadcrumb-bg breadcrumb-bg-about py-5">
        <div class="container py-lg-5 py-md-3">
            <h2 class="title">About Us</h2>
        </div>
    </div>
</section>
<section class="w3l-breadcrumb">
    <div class="container">
        <ul class="breadcrumbs-custom-path">
            <li><a href="#url">Home</a></li>
            <li class="active"><span class="fa fa-arrow-right mx-2" aria-hidden="true"></span> About Us</li>
        </ul>
    </div>
</section>
<section class="w3l-about py-5" id="about">
    <div class="container py-lg-5 py-md-3">
        <div class="about-block">
            <h6 class="title-small">About company</h6>
            <h3 class="title-big">We are qualified & of experience in this field.</h3>
            <p class="mt-md-4 mt-3">
                Mechanic Workshop fixes vehicles and replaces their parts for customers. 
                Their duties include repairing the vehicle's mechanical components, 
                diagnosing problems with cars and performing maintenance work on them as well.
            </p>
            <a href="#url" class="img-height">
                <img src="~/assets/images/ab.jpg" alt="" class="radius-image img-fluid mt-md-5 mt-4">
            </a>
        </div>
    </div>
    
</section>
<section class="w3l-bottom-grids-6 py-5" id="who">
    <div class="container py-lg-5 py-md-4">
        <div class="grid-alignment mb-md-5 mb-4">
            <div class="grid-info">
                <h6 class="title-small">Who we are</h6>
                <h3 class="title-big">Excellece in automotive services since 2011</h3>
            </div>
            <div class="grid-btn text-lg-right">
                <a href="@Url.Action("Create","BookAppointments")" class="btn btn-style btn-primary">
                    Free Consultation
                    <span class="fa fa-long-arrow-right"></span>
                </a>
            </div>
        </div>
        <div class="grids-area-hny main-cont-wthree-fea row pt-lg-4">
            <div class="col-lg-4 col-md-6 grids-feature">
                <div class="area-box">
                    <span class="fa fa-car"></span>
                    <div class="info">
                        <h4><a href="#feature" class="title-head">Brake fluid exchange</a></h4>
                        <p>
                            Brakes are one of the most critical aspects of any automobile. 
                            This include repairing and servicing every part of the brake system.
                        </p>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-6 grids-feature mt-md-0 mt-4">
                <div class="area-box">
                    <span class="fa fa-plug"></span>
                    <div class="info">
                        <h4><a href="#feature" class="title-head">Wheel alignment</a></h4>
                        <p>
                            A reliable general auto service provider will evaluate, rotate, 
                            and perform the balancing of tires as per the maintenance schedule.
                        </p>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-6 grids-feature mt-lg-0 mt-4">
                <div class="area-box">
                    <span class="fa fa-wrench"></span>
                    <div class="info">
                        <h4><a href="#feature" class="title-head">Maintenance packages</a></h4>
                        <p>
                            Car diagnostic tests scan your car's components and systems to check for issues with 
                            components like the engine, transmission, oil tank, throttle, and many more
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- about block3 -->
<section class="w3l-content-4 py-5" id="features">
    <div class="content-4-main py-lg-5 py-md-4">
        <div class="container">
            <div class="content-info-in row">
                <div class="content-left col-lg-6">
                    <h3 class="title-small"> What we do</h3>
                    <h3 class="title-big">Our auto shop technicians are dedicated to helping you get back on the road.</h3>
                    <p class="mt-4">
                        Mechanic Workshop fixes vehicles and replaces their parts for customers. Their duties include repairing 
                        the vehicle's mechanical components, diagnosing problems with cars and performing maintenance work on them as well.
                    </p>
                    
                    <img src="~/assets/images/banner3.jpg" class="img-fluid mt-3" alt="">
                </div>
                <div class="content-right col-lg-6 pl-lg-4 mt-lg-0 mt-md-5 mt-3">
                    <div class="row content4-right-grids mb-lg-5 mb-4">
                        <div class="col-2 content4-right-icon">
                            <div class="content4-icon">
                                <span class="fa fa-cogs"></span>
                            </div>
                        </div>
                        <div class="col-10 content4-right-info">
                            <h6><a href="#url">Free diagnostics.</a></h6>
                            
                        </div>
                    </div>
                    <div class="row content4-right-grids mb-lg-5 mb-4">
                        <div class="col-2 content4-right-icon">
                            <div class="content4-icon">
                                <span class="fa fa-wrench"></span>
                            </div>
                        </div>
                        <div class="col-10 content4-right-info">
                            <h6><a href="#url"> Fast repair.</a></h6>
                            
                        </div>
                    </div>
                    <div class="row content4-right-grids">
                        <div class="col-2 content4-right-icon">
                            <div class="content4-icon">
                                <span class="fa fa-clock-o"></span>
                            </div>
                        </div>
                        <div class="col-10 content4-right-info">
                            <h6><a href="#url"> 90 days warranty</a></h6>
                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- //about block3 -->
<section class="w3l-about4 py-5">
    <div class="container px-0 py-lg-5 py-md-4">
        <div class="history-info position-relative">
            <div class="heading ml-auto">
                <h3 class="title-big">We’ll make your car feel brand new visit our workshop today.</h3>
                
                <a href="#small-dialog" class="popup-with-zoom-anim play-view text-center mt-4">
                    <span class="video-play-icon">
                        <span class="fa fa-play"></span>
                    </span>
                </a>
            </div>
            <div class="position-relative">
                <!-- dialog itself, mfp-hide class is required to make dialog hidden -->
                <div id="small-dialog" class="zoom-anim-dialog mfp-hide">
                    <iframe src="https://player.vimeo.com/video/169036704" frameborder="0" allow="autoplay; fullscreen" allowfullscreen></iframe>
                </div>
                <!-- popup -->
                <div id="popup-video" class="pop-overlay">
                    <div class="popup">
                        <div class="align-popup-video">
                            <iframe src="https://www.youtube.com/embed/JemJbNJ4ZtU" frameborder="0"
                                    allowfullscreen></iframe>
                            <a class="close" href="#portfolio">&times;</a>
                        </div>
                    </div>
                </div>
                <!-- /popup -->
            </div>
        </div>
    </div>
</section>
